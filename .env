# Default config values - tracked in git
# For secrets, create .env.local (gitignored) which overrides these

# NATS
NATS_PORT=4222
NATS_URL=nats://localhost:4222

# Voxel visualizer
VOXEL_PORT=5173

# Narun HTTP/gRPC gateway
NARUN_PORT=8081
NARUN_METRICS_PORT=9091

# Process Compose
PC_PORT=8080

# nats2sse (NATS to SSE bridge for Datastar)
NATS2SSE_PORT=8083

# selfhostmap (local map tiles)
SELFHOSTMAP_PORT=8082

# Secrets go in .env.local (not tracked):
#   NATS_USER=myuser
#   NATS_PASSWORD=mypassword
#   NATS_TOKEN=mytoken
